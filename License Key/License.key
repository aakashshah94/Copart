namespace ConsoleApplication11
{
    class Program
    {
        static void Main(string[] args)
        {
            String s = Console.ReadLine();                  //reading the string containing licence key
            int k = Convert.ToInt32(Console.ReadLine());    //readng the num of char allowed in each part
            
            String answer = "";                           //the string to store the anwer
            int m = 0;                                   //for checking each part length
            int length = s.Length;
            for (int i = 0; i <length; i++)              //iterate through string
            {
                if (!(s[i] == '-'))                      //if - than ignore
                {
                    m++;
                    if (!(char.IsDigit(s[i])))             //if alphbet covert to upper case
                    {
                        answer = answer + char.ToUpper(s[i]);
                    }
                    else                                   //if number directly add
                    {
                        answer = answer + s[i];
                    }

                }
                if (m == k && i<length-1 )           //if length of one segment reached append "-" except at the end
                {
                    answer = answer + "-";
                    m = 0;
                }


            }
            Console.WriteLine(answer);              //print answer to console
            Console.ReadKey();
        }
    }
}